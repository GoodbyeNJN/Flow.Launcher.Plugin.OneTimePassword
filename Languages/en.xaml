    <system:String x:Key="flowlauncher_plugin_otp_editor_form_uri_empty_warning">URI field should not be empty</system:String>
    <system:String x:Key="flowlauncher_plugin_otp_editor_form_label_empty_warning">Label field should not be empty</system:String>

