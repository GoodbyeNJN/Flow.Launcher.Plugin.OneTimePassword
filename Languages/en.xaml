<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="flowlauncher_plugin_otp_plugin_title">One-time Password</system:String>
    <system:String x:Key="flowlauncher_plugin_otp_plugin_description">Manage and generate time-based one-time passwords</system:String>

    <system:String x:Key="flowlauncher_plugin_otp_main_empty_title">One-time password configuration is empty</system:String>
    <system:String x:Key="flowlauncher_plugin_otp_main_empty_sub_title">Please add configuration in plugin setting first</system:String>
    <system:String x:Key="flowlauncher_plugin_otp_main_expired">Expired in</system:String>
    <system:String x:Key="flowlauncher_plugin_otp_main_copy">Click to copy</system:String>

    <system:String x:Key="flowlauncher_plugin_otp_setting_add_button">Add</system:String>
    <system:String x:Key="flowlauncher_plugin_otp_setting_edit_button">Edit</system:String>
    <system:String x:Key="flowlauncher_plugin_otp_setting_delete_button">Delete</system:String>
    <system:String x:Key="flowlauncher_plugin_otp_setting_delete_warning">Are you sure you want to delete {0}?</system:String>

    <system:String x:Key="flowlauncher_plugin_otp_editor_title">Edit Configuration</system:String>
